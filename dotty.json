{
    "directories": ["~/.config/systemd/user",
		    "~/.erc/logs",
		    "~/.config/mimi",
		    "~/.emacs.d",
		    "~/.i3"],
    "link": {
	"dunstrc"			: "~/.dunstrc",
	"zshrc"				: "~/.zshrc",
	"youtube-dl.conf"		: "~/.youtube-dl.conf",
	"livestreamerrc"		: "~/.livestreamerrc",
	"mime.conf"			: "~/.config/mimi/mime.conf",
	"x11/xinitrc"			: "~/.xinitrc",
	"x11/xprofile"			: "~/.xprofile",
	"x11/Xmodmap"			: "~/.Xmodmap",
	"x11/Xresources"		: "~/.Xresources",
	"emacs/lisp"                    : "~/.emacs.d/lisp",
	"emacs/init.el"                 : "~/.emacs.d/init.el",
	"emacs/firstrun.el"             : "~/.emacs.d/firstrun.el",
	"gitconfig"                     : "~/.gitconfig",
	"i3config"                      : "~/.i3/config"
    },
    "copy": {
	"offlineimaprc" : "~/.offlineimaprc"
    },
    "commands": ["emacs -batch -Q -l ~/.emacs.d/firstrun.el",
		 "touch ~/.emacs.d/custom.el",
		 "touch ~/.emacs.d/lisp/config-twitch.el"
		],
    "pacman": ["zsh", "i3-wm", "i3lock", "emacs", "xorg-xinit", "xorg-xkill",
	       "xorg-xrandr", "xorg-xrdb", "feh", "pcmanfm", "dmenu", "i3status",
	       "xfce4-terminal", "dunst", "notification-daemon", "elinks",
	       "ttf-inconsolata", "xorg-xinput", "gcc", "clang", "go"]
}
